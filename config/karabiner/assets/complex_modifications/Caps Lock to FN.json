{
  "title": "Caps Lock to FN",
  "rules": [
    {
      "description": "Change Caps Lock to FN",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "fn"
            }
          ]
        }
      ]
    }
  ]
}
